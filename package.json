{
  "name": "ember-react-components",
  "version": "3.0.2",
  "description": "React component support in Ember",
  "keywords": [
    "ember-addon",
    "decorator",
    "react"
  ],
  "repository": "https://github.com/alexlafroscia/ember-react-components",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "homepage": "https://alexlafroscia.com/ember-react-components",
  "scripts": {
    "build": "ember build --environment=production",
    "lint:js": "eslint --ext js .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "7.14.3",
    "ember-auto-import": "1.11.3",
    "ember-cli-babel": "7.26.6",
    "ember-cli-babel-plugin-helpers": "^1.0.2",
    "ember-cli-htmlbars": "^4.2.2",
    "ember-cli-version-checker": "5.1.2",
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@ember/edition-utils": "^1.1.1",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.6",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.0.3",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.16.2",
    "ember-cli-addon-docs": "^3.0.0",
    "ember-cli-addon-docs-yuidoc": "^1.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-decorators": "^5.1.2",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-metrics": "^1.2.0",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^7.0.0",
    "ember-sinon-qunit": "^6.0.0",
    "ember-source": "~3.16.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.4.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-ember": "^7.7.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^1.3.1",
    "lint-staged": "^7.2.0",
    "loader.js": "^4.7.0",
    "prettier": "2.3.0",
    "qunit": "^2.15.0",
    "qunit-dom": "^1.6.0",
    "standard-version": "^8.0.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
